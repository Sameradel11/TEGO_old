{
    "version":2,
    "builds":[
        {
            "src":"tego/wsgi.py",
            "use": "@vercel/python",
            "config":{
                "maxLamdaSize":"15mb"
            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"tego/wsgi.py"
        }  
    ]
}